{
  "triggers": [
    {
      "name": "House East",
      "watchPattern": "/aiinput/house_e_snap*.jpg",
      "enabled": true,
      "threshold": {
        "minimum": 75,
        "maximum": 100
      },
      "handlers": {
        "webRequest": {
          "triggerUris": ["https://tbsurveillance.duckdns.org/admin?trigger&camera=house_east&user={{blueIris.username}}&pw={{blueIris.password}}"]
